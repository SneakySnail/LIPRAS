function num = fnstr2num(fcnNames)
num = [];

for i = 1:length(fcnNames)
	if strcmpi(fcnNames{i}, 'Gaussian')
		num = [num, 1];
	elseif strcmpi(fcnNames{i}, 'Lorentzian')
		num = [num, 2];
	elseif strcmpi(fcnNames{i}, 'PearsonVII')
		num = [num, 3];
	elseif strcmpi(fcnNames{i}, 'PsuedoVoigt')
		num = [num, 4];
	elseif strmpi(fcnNames{i}, 'AsymmetricPVII')
		num = [num, 5];
	else
		num = nan;
	end
end